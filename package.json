{
  "name": "@l9/pipedream",
  "version": "0.32.14",
  "description": "PipeDream - Maintainable Script Automation",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "goBinary": {
    "name": "pipedream",
    "path": "./bin",
    "url": "https://github.com/Layer9Berlin/PipeDream/releases/download/v{{version}}/npm_pipedream_{{version}}_{{platform}}_{{arch}}.tar.gz"
  },
  "files": [
    "include"
  ],
  "scripts": {
    "test": "go test src",
    "postinstall": "go-npm install && ln -Ffs $(npm get prefix)/lib/node_modules/@l9/pipedream/include/pipedream_pipes $(npm bin)  && ln -fs $(npm bin)/pipedream $(npm bin)/p",
    "preuninstall": "go-npm uninstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Layer9Berlin/PipeDream.git"
  },
  "keywords": [
    "pipedream",
    "script",
    "automation",
    "shell",
    "terminal",
    "command",
    "line"
  ],
  "author": "Layer9 GmbH",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Layer9Berlin/PipeDream/issues"
  },
  "homepage": "https://github.com/Layer9Berlin/PipeDream#readme",
  "dependencies": {
    "go-npm": "^0.1.9"
  }
}
